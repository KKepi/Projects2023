posloupnost = [0,1]                                              # posloupnost zacina prvky 0, 1

delka_posloupnosti = int(input("zadejte delku posloupnosti: "))

for i in range(1, delka_posloupnosti):                   # dulezite si uvedomit, ze Fibonacciho posloupnost primo a krasne nahrava, k tomuto kratkemu reseni.             
    cislo = posloupnost[i] + posloupnost[i-1]            # i = 1 (zacatek)
    posloupnost.append(cislo)                            # posloupnost[1] (druhy prvek)
                                                         # posloupnost[1-1] = posloupnost[0] (prvni prvek)
print(posloupnost)                                       # 
                                                         # nove vytvoreny prvek je soucet techto vypoctu
                                                         # a jestli, ze se chceme posunout dal, pak           i = 2 ... posloupnost[2] nove pridany vypocet 
                                                         #                                                              posloupnost[1] prvek pred nove vypocitanym prvkem
